@prefix esadm: <http://vocab.linkeddata.es/datosabiertos/def/sector-publico/territorio#> .


iso19156_OB:OM_Observation.metadata a owl:ObjectProperty ;
    rdfs:label "metadata" ;
    iso19150-2:associationName "Metadata" ;
    rdfs:domain iso19156_OB:OM_Observation ;
    rdfs:isDefinedBy "http://standards.iso.org/iso/19156/ed-1/en/" ;
    rdfs:range iso19115_503:MD_Metadata .

geo:rcc8dc
	a rdf:Property, owl:ObjectProperty ;
	rdfs:domain geo:SpatialObject ;
	rdfs:range geo:SpatialObject ;
	owl:equivalentProperty geo:sfDisjoint, geo:ehDisjoint ;
	rdfs:isDefinedBy 
		<http://www.opengis.net/ont/geosparql#> , 
		<http://www.opengis.net/spec/geosparql/1.0/req/topology-vocab-extension/rcc8-spatial-relations> , 
		<http://www.opengis.net/spec/geosparql/1.1/req/topology-vocab-extension/rcc8-spatial-relations> ;
	skos:definition """States that the subject SpatialObject is spatially disjoint from the object SpatialObject. DE-9IM: FFTFFTTTT"""@en ;
	rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
	skos:prefLabel "disconnected"@en .

geo:ehDisjoint
	a rdf:Property, owl:ObjectProperty ;
	owl:equivalentProperty geo:sfDisjoint ;
	rdfs:domain geo:SpatialObject ;
	rdfs:range geo:SpatialObject ;
	owl:equivalentProperty geo:sfDisjoint, geo:rcc8dc ;
	owl:propertyDisjointWith  geo:sfIntersects ;
	rdfs:isDefinedBy
		<http://www.opengis.net/ont/geosparql#> ,
		<http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/eh-spatial-relations> ,
		<http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/eh-spatial-relations> ;		
	skos:definition """States that the subject SpatialObject is spatially disjoint from the object SpatialObject. DE-9IM: FF*FF****"""@en ;
	rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
	skos:prefLabel "disjoint"@en .

geo:sfDisjoint
	a rdf:Property, owl:ObjectProperty ;
	owl:equivalentProperty geo:ehDisjoint ;
	rdfs:domain geo:SpatialObject ;
	rdfs:range geo:SpatialObject ;
	owl:equivalentProperty geo:ehDisjoint, geo:rcc8dc ;
	owl:propertyDisjointWith  geo:sfIntersects ;
	rdfs:isDefinedBy 
		<http://www.opengis.net/ont/geosparql#> , 
		<http://www.opengis.net/spec/geosparql/1.0/req/topology-vocab-extension/sf-spatial-relations> ,
		<http://www.opengis.net/spec/geosparql/1.1/req/topology-vocab-extension/sf-spatial-relations> ;
	skos:definition """States that the subject SpatialObject is spatially disjoint from the object SpatialObject. DE-9IM: FF*FF****"""@en ;
	rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
	skos:prefLabel "disjoint"@en .

geo:sfIntersects
	a rdf:Property, owl:ObjectProperty ;
	rdfs:domain geo:SpatialObject ;
	rdfs:range geo:SpatialObject ;
	owl:propertyDisjointWith  geo:sfDisjoint, geo:ehDisjoint ;
	rdfs:isDefinedBy 
		<http://www.opengis.net/ont/geosparql#> , 
		<http://www.opengis.net/spec/geosparql/1.0/req/topology-vocab-extension/sf-spatial-relations> ,
		<http://www.opengis.net/spec/geosparql/1.1/req/topology-vocab-extension/sf-spatial-relations> ;
	skos:definition """States that the subject SpatialObject is not spatially disjoint from the object SpatialObject. DE-9IM: T******** ^ *T******* ^ ***T***** ^ ****T****"""@en ;
	rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
	skos:prefLabel "intersects"@en .

time:inXSDgYear
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Position of an instant, expressed using xsd:gYear"@en ;
  rdfs:domain time:Instant ;
  rdfs:label "in XSD g-Year"@en ;
  rdfs:range xsd:gYear ;
  skos:definition "Position of an instant, expressed using xsd:gYear"@en .

dc:contributor a owl:AnnotationProperty .

dc:creator a owl:AnnotationProperty .

dc:license a owl:AnnotationProperty .

dc:title a owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

agrixel: a owl:Ontology ;
    dc:contributor "Cecilio Angulo" ;
    dc:creator "Joan Van Eeckhout Alsinet",
        "Miona Dimic" ;
    dc:license <https://creativecommons.org/licenses/by-nc-nd/4.0/> ;
    dc:title "Agrixel Ontology" ;
    owl:imports <http://qudt.org/3.1.1/vocab/quantitykind>,
        <http://vocab.linkeddata.es/datosabiertos/def/sector-publico/territorio>,
        <http://w3id.org/glosis/model>,
        <https://def.isotc211.org/ontologies/iso19115/2006/MetadataEntitySetInformation.rdf>,
        <https://w3id.org/demeter/agri> ;
    owl:versionInfo "1.0.0" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

agrixel:siexConcept a owl:ObjectProperty ;
    rdfs:label "siex concept" ;
    rdfs:range [ a owl:Class ;
            owl:intersectionOf ( skos:Concept [ a owl:Class ;
                        owl:unionOf ( [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexFertilizerMaterialValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexIntendedCropUseValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexIrrigationWaterSourceValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexRootstockValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexSoilCoverActivityValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexSoilCoverTypeValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexTrellisingSystemValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexFarmingSystemValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexCropTypeValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexCropProductValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexFertilizationMethodValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue siex_cl:siexFertilizationTypeValueCode ;
                                    owl:onProperty skos:inScheme ] ) ] ) ] .

agrixel:Agrixel a owl:Class ;
    rdfs:label "Agrixel" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty geo:sfIntersects ;
            owl:someValuesFrom agrixel:CropArea ],
        [ a owl:Restriction ;
            owl:onClass agrixel:Parcel ;
            owl:onProperty s4agri:isContainedIn ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass geo:Geometry ;
            owl:onProperty geo:hasGeometry ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        sosa:FeatureOfInterest .

agrixel:sigpacConcept a owl:ObjectProperty ;
    rdfs:label "sigpac concept" ;
    rdfs:range [ a owl:Class ;
            owl:intersectionOf ( skos:Concept [ a owl:Class ;
                        owl:unionOf ( [ a owl:Restriction ;
                                    owl:hasValue sigpac_cl:sigpacIncidentValueCode ;
                                    owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                    owl:hasValue sigpac_cl:sigpacLandUseValueCode ;
                                    owl:onProperty skos:inScheme ] ) ] ) ] .

siex_cl:siexCropProductValueCode a owl:NamedIndividual .

siex_cl:siexCropTypeValueCode a owl:NamedIndividual .

siex_cl:siexFarmingSystemValueCode a owl:NamedIndividual .

siex_cl:siexFertilizationMethodValueCode a owl:NamedIndividual .

siex_cl:siexFertilizationTypeValueCode a owl:NamedIndividual .

siex_cl:siexFertilizerMaterialValueCode a owl:NamedIndividual .

siex_cl:siexIntendedCropUseValueCode a owl:NamedIndividual .

siex_cl:siexIrrigationWaterSourceValueCode a owl:NamedIndividual .

siex_cl:siexRootstockValueCode a owl:NamedIndividual .

siex_cl:siexSoilCoverActivityValueCode a owl:NamedIndividual .

siex_cl:siexSoilCoverTypeValueCode a owl:NamedIndividual .

siex_cl:siexTrellisingSystemValueCode a owl:NamedIndividual .

agrixel:CropArea a owl:Class ;
    rdfs:label "Crop Area" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty geo:sfIntersects ;
            owl:someValuesFrom agrixel:ParcelUnit ],
        [ a owl:Restriction ;
            owl:onClass geo:Geometry ;
            owl:onProperty geo:hasGeometry ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass agrixel:Parcel ;
            owl:onProperty s4agri:isContainedIn ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        sosa:FeatureOfInterest .

agrixel:ParcelUnit a owl:Class ;
    rdfs:label "Parcel Unit" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass agrixel:Parcel ;
            owl:onProperty s4agri:isContainedIn ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass geo:Geometry ;
            owl:onProperty geo:hasGeometry ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( skos:Concept [ a owl:Class ;
                                owl:unionOf ( [ a owl:Restriction ;
                                            owl:hasValue sigpac_cl:sigpacLandUseValueCode ;
                                            owl:onProperty skos:inScheme ] [ a owl:Restriction ;
                                            owl:hasValue sigpac_cl:sigpacIncidentValueCode ;
                                            owl:onProperty skos:inScheme ] ) ] ) ] ;
            owl:onProperty agrixel:sigpacConcept ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Restriction ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty time:inXSDgYear ] ;
            owl:onProperty time:hasTime ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty http:identifier ],
        sosa:FeatureOfInterest .

sigpac_cl:sigpacIncidentValueCode a owl:NamedIndividual .

sigpac_cl:sigpacLandUseValueCode a owl:NamedIndividual .

agrixel:Parcel a owl:Class ;
    rdfs:label "Parcel" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass agrixel:ParcelUnit ;
            owl:onProperty s4agri:contains ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass agrixel:CropArea ;
            owl:onProperty s4agri:contains ],
        [ a owl:Restriction ;
            owl:onClass s4agri:Farm ;
            owl:onProperty s4agri:isContainedIn ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass agrixel:Agrixel ;
            owl:onProperty s4agri:contains ] .
